<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Ofnifile.Views.TabFeed.TabFeedControl">

  <Grid>
    <TabControl Theme="{StaticResource TabControlBase}" Margin="5">
      <TabItem Theme="{StaticResource TabItemBase}" Header="Main tab">
        <TextBlock Margin="5" Text="Main tab text"/>
      </TabItem>
      
      <TabItem Theme="{StaticResource TabItemBase}" Header="View tab">
        <TextBlock Margin="5" Text="View tab text"/>
      </TabItem>
    </TabControl>
  </Grid>
  
</UserControl>
